<?xml version="1.0"?>
<prefigure xmlns="https://prefigure.org" label="sqrts" authored-label="">
<diagram dimensions="(400,400)" margins="50">
  <!-- Reference to containing ancestor comes from PreTeXt context     -->
  <!-- Serial number is from document-wide count of PreFigure diagrams -->
  <caption>Figure 7 (1)</caption>
  <definition>a=4</definition>
  <definition>b=9</definition>
  <definition>f(x) = x^(0.5)</definition>
  <coordinates bbox="[0,0,10,4]">
    <grid-axes xlabel="x" ylabel="y"/>
    <group>
      <graph function="f"/>
      <tangent-line function="f" point="a" infinite="yes"/>
      <point p="(a, f(a))" stroke="red">
      </point>
        <label anchor="(4,1.75)">
        <m>(4,2)</m>
        </label>
        <label anchor="(7,2.25)">
        <m>f(x) = \sqrt{x}</m>
        </label>
    </group>
    <group>
      <tangent-line function="f" point="b" infinite="yes" stroke="green"/>
      <point p="(b, f(b))" stroke="green" fill="green">
      </point>
      <label anchor="(9,2.75)">
        <m>(9,3)</m>
      </label>
    </group>
  </coordinates>
</diagram>
</prefigure>
